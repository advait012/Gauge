node {
        stage('clone sources') {
                git url: 'https://github.com/advait012/Gauge.git'
        }
        
        stage('SonarQube Analysis'){
         cobertura coberturaReportFile: '**/cobertura-coverage.xml'
         def scannerHome = tool 'SonarScanner';
          withSonarQubeEnv() {
            bat "${scannerHome}/bin/sonar-scanner"
            }
       } 
}
